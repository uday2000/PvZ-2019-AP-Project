<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.Pane?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="224.0"
      prefWidth="352.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.Controller">
    <children>

        <ImageView fitHeight="448.0" fitWidth="704.0" pickOnBounds="true" preserveRatio="true" smooth="false">
            <Image url="@sprites/bgs/title screen.png"/>
        </ImageView>

        <Button fx:id="playButton" layoutX="290.0" layoutY="261.0" maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity"
                minWidth="-Infinity" mnemonicParsing="false" onMouseEntered="#playAction" onMouseExited="#playRelease" prefHeight="40.0"
                prefWidth="124.0">
            <graphic>
                <ImageView fx:id="playButtonImage" fitHeight="51.0" fitWidth="126.0" pickOnBounds="true"
                           preserveRatio="true"
                           smooth="false">
                    <Image url="@sprites/buttons/playbutton.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button layoutX="288.0" layoutY="319.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                minWidth="-Infinity" mnemonicParsing="false" onMouseEntered="#exitAction" onMouseExited="#exitReset"  onMouseClicked="#exitGame" prefHeight="49.0"
                prefWidth="124.0">
            <graphic>
                <ImageView fx:id="exitButtonImage" fitHeight="57.0" fitWidth="132.0" pickOnBounds="true" preserveRatio="true" smooth="false">
                    <Image url="@sprites/buttons/exitbutton.png"/>
                </ImageView>
            </graphic>
        </Button>
    </children>
</Pane>
